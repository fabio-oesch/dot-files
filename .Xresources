!URxvt.font: xft:SauceCodePro Nerd Font:size=10:regular:hinting=true:antialias=true,xft:FontAwesome:size=10:regular
!URxvt.font: xft:Inconsolata Nerd Font Mono:size=11:medium:hinting=true:antialias=true,xft:FontAwesome:size=10:regular
!URxvt.boldfont: xft:Droid Sans Mono for Powerline:size=10:bold:hinting=true:antialias=true
URxvt*font:  xft:inconsolata:size=10:antialias=true,xft:DejaVu Sans:size=10:antialias=true,xft:Sauce Code Powerline:antialias=true:hinting=true:size=9


URxvt.letterSpace: 0

URxvt.internalBorder:      0
URxvt.externalBorder:      0

URxvt.scrollBar:           false

URxvt*fading:              30

URxvt.perl-ext-common:     default,matcher,resize-font,fullscreen

URxvt.keysym.C-Delete:    perl:matcher:last
URxvt.keysym.M-Delete:    perl:matcher:list
URxvt.matcher.button:     1
URxvt.matcher.pattern.1:  \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt.matcher.pattern.2:  \\B(/\\S+?):(\\d+)(?=:|$)
URxvt.matcher.launcher.2: gvim +$2 $1

URxvt.keysym.F11:          perl:fullscreen:switch

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger:  C-Up

URxvt.url-launcher:        /usr/bin/firefox
URxvt.colorUL:             #7780a1

! not mix bright with bold
URxvt.intensityStyles: false

