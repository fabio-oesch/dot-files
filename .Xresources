! Window opacity
rofi.opacity:                        100
! Window width
rofi.width:                          60
! Number of lines
rofi.lines:                          7
! Number of columns
rofi.columns:                        1
! Font to use
rofi.font:                           Sauce Code Powerline 15


rofi.color-enabled:                  true
! Color scheme window
! rofi.color-window: argb:c336343f, argb:0e9dbdb, argb:0ef6155
! background, border, separator color
rofi.color-window: argb:c3282828, argb:c3d65d0e, argb:c3ebdbb2
! Color scheme for normal row
! rofi.color-normal: argb:0191525,argb:dcffffff, argb:02f1e2e, argb:9a0e1c26, {{ primary }}
! background, foreground, background alt, highlight background, highlight foreground
rofi.color-normal: argb:c3282828, argb:c3ebdbb2, argb:c332302f, argb:c3504945, argb:c3ebdbb2
! Color scheme for active row
! rofi.color-active: argb:02f1e2e, #268bd2, argb:02f1e2e, #268bd2, #fdf6e3
! background, foreground, background alt, highlight background, highlight foreground
rofi.color-active: argb:c3282828, argb:c3ebdbb2, argb:c332302f, argb:c3504945, argb:c3ebdbb2
! Color scheme for urgent row
! rofi.color-urgent: argb:0fdf6e3, #dc322f, argb:0eee8d5, #dc322f, #fdf6e3
! background, foreground, background alt, highlight background, highlight foreground
rofi.color-urgent: argb:c3282828, argb:c3ebdbb2, argb:c332302f, argb:c3504945, argb:c3ebdbb2
